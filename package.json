{
  "name": "inc-dec",
  "main": "./lib/inc-dec",
  "version": "1.2.0",
  "description": "Change word case, number value and loop between keywords with the arrow keys.",
  "keywords": [
    "inc-dec",
    "increment",
    "decrement",
    "lowercase",
    "capitalize",
    "uppercase"
  ],
  "activationCommands": {
    "atom-text-editor:not([mini])": [
      "inc-dec:inc",
      "inc-dec:dec"
    ]
  },
  "repository": "https://github.com/yboyer/atom-inc-dec",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {}
}
